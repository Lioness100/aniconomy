{
  "name": "aniconomy",
  "version": "1.0.0",
  "description": "Bot made for Fiverr user chrisoliverii30",
  "main": "dist/index.js",
  "repository": "https://github.com/Lioness100/aniconomy",
  "author": "Lioness100 <jchickmm2@gmail.com>",
  "license": "MIT",
  "type": "module",
  "private": true,
  "imports": {
    "#lib/*": "./dist/lib/*.js",
    "#env": "./dist/lib/env/index.js",
    "#root/*": "./dist/*.js",
    "#ext": "./dist/lib/extensions/index.js",
    "#ext/*": "./dist/lib/extensions/*.js",
    "#structures/*": "./dist/lib/structures/*.js",
    "#types/*": "./dist/lib/types/*.js",
    "#entities/*": "./dist/lib/database/entities/*.js"
  },
  "scripts": {
    "start": "node -r dotenv/config --experimental-json-modules .",
    "lint": "eslint src --ext ts --fix",
    "format": "prettier --write --loglevel=warn \"src/**/*.ts\"",
    "update": "yarn upgrade-interactive --latest",
    "build": "node --experimental-json-modules scripts/build.mjs ",
    "watch": "node scripts/watch.mjs",
    "clean": "node scripts/clean.mjs",
    "setup": "run-s clean build start",
    "typecheck": "tsc -p src --noEmit"
  },
  "dependencies": {
    "@androz2091/discord-invites-tracker": "0.1.2",
    "@sapphire/decorators": "2.2.0",
    "@sapphire/discord.js-utilities": "2.0.2",
    "@sapphire/fetch": "1.0.5",
    "@sapphire/framework": "1.0.2",
    "@sapphire/stopwatch": "1.1.4",
    "@sapphire/type": "2.0.11",
    "@sapphire/utilities": "2.0.1",
    "@skyra/editable-commands": "1.0.0",
    "@typegoose/typegoose": "8.1.1",
    "colorette": "1.2.2",
    "colorthief": "^2.3.2",
    "discord-api-types": "0.20.2",
    "discord-blackjack": "2.0.4",
    "discord-economy-super": "1.3.6",
    "discord-giveaways": "4.5.1",
    "discord-voice": "^2.0.3",
    "discord.js": "12.5.3",
    "dotenv": "10.0.0",
    "mal-scraper": "^2.11.3",
    "millify": "^4.0.0",
    "mongoose": "5.13.6",
    "ms": "2.1.3",
    "node-myanimelist": "^3.0.4",
    "tslib": "2.3.0"
  },
  "devDependencies": {
    "@lioness100/eslint-config": "^1.0.9",
    "@lioness100/prettier-config": "^1.0.0",
    "@lioness100/ts-config": "^1.0.3",
    "@types/ms": "^0.7.31",
    "@types/node": "^16.4.0",
    "esbuild": "^0.12.15",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.3.4"
  },
  "engines": {
    "node": ">=14",
    "npm": ">=6"
  },
  "prettier": "@lioness100/prettier-config"
}
